<lemma>
<title>ch8_circ_arbiter.arb_m0.cir1_inv6_INV</title>
<origin>ch8_circ_arbiter | arb_m0 | cir1/inv6/INV</origin>
<theories>
   <theory name="integer"/>
   <theory name="linear_arith"/>
 </theories>
<typenv>
   <variable name="a1" type="ℤ"/>
   <variable name="r1" type="ℤ"/>
 </typenv>
<hypothesis needed="true">a1 ≤ r1</hypothesis>
<hypothesis needed="true">r1 ≤ a1+1</hypothesis>
